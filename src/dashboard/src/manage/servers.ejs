<%- include("../parts/head") %>

<title>Servers Â» <%= bot.user.username %> Dashboard</title>

<div class="text-center text-light" style="padding-top: 65px; padding-bottom: 20px;">
    <div class="p-5 text-center" style="padding-top: 110px; background-color: #3B465E;">
        <h1 class="mb-3">Select a Server...</h1>
    </div>
</div>
 <div class="row">
     <ul class="list-group" style="padding-bottom: 20px">
        <% guilds.sort((a, b) => b.name.length - a.name.length) ;%>
         <% for (let i = 0; i < guilds.length; i++) { %>
               <% if(bot.guilds.cache.get(guilds[i].id)) {%>
                  <li class="list-group-item"><a class="text-dark" href="/server/<%= guilds[i].id %>/home">
                    <% if (!guilds[i].icon) { %>
                        <img class="rounded-circle" width="84px" style="opacity: 1;" src="data:image/png;base64,"> 
                    <% } else { %>
                        <img class="rounded-circle" width="84px" style="opacity: 1;" src="https://cdn.discordapp.com/icons/<%= guilds[i].id %>/<%= guilds[i].icon %>"> 
                    <% } %>
                        <span style="padding-left: 20px; font-size: larger;"><%= guilds[i].name %></span>
                        <span style="color: GREEN">(Manage)</span>
                    </a>
                  </li>
              <%} else {%> 
                  <li class="list-group-item"><a class="text-dark" href="/server/<%= guilds[i].id %>/invite">
                    <% if (!guilds[i].icon) { %>
                        <img class="rounded-circle" width="84px" style="opacity: 1;" src="data:image/png;base64,"> 
                    <% } else { %>
                        <img class="rounded-circle" width="84px" style="opacity: 1;" src="https://cdn.discordapp.com/icons/<%= guilds[i].id %>/<%= guilds[i].icon %>"> 
                    <% } %>
                        <span style="padding-left: 20px; font-size: larger;"><%= guilds[i].name %></span>
                        <span style="color: RED">(Invite)</span>
                    </a>
                  </li>
             <% } %>
         <% } %>
     </ul>
</div>
<%- include("../parts/foot") %>