<div id="load_screen">
  <div class="loader">
    <div class="loader-content">
        <div class="spinner-grow align-self-center">
      </div>
    </div>
  </div>
 </div>
 <nav class="navbar navbar-expand-lg fixed-top shadow navbar-dark" style="background-color: #7289da;">
 
   <div class="container">
            <a class="navbar-brand" href="/">
                <img src="/static/FullLogo.png" width="auto" height="40" alt="" loading="lazy" />
            </a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fas fa-bars"></i>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="/">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/stats">Stats</a>
                    </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/source">My Source</a>
                    </li>
                   <li class="nav-item">
                        <a class="nav-link" href="/dc">My Server</a>                     
                  </li>
                  <li class="nav-item">
                        <a class="nav-link" href="/invite">Invite Me</a>                     
                  </li>
                </ul>
                <% if (user) { %>
                    <ul class="navbar-nav">
                        <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle d-flex align-items-center" href="#" id="navbarDropdownMenuLink" role="button" data-toggle="dropdown" aria-expanded="false">
                            <img src="https://cdn.discordapp.com/avatars/<%= user.id %>/<%= user.avatar %>" class="rounded-circle" height="42" alt="Avatar"/> <span style="padding-left: 10px;"><%= user.username %>#<%= user.discriminator %></span>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <li><h6 class="dropdown-header">Menu</h6></li>
                            <li><a class="dropdown-item" href="/servers"><i class="fas fa-server"></i> My Servers</a></li>
                            <li><a class="dropdown-item" href="/profile"><i class="fas fa-id-card"></i> My Profile</a></li>
                            <hr>
                            <li><a class="dropdown-item" href="/logout"><i class="fas fa-sign-out-alt"></i> Log Out</a></li>
                        </ul>
                        </li>
                    </ul>
                <% } else { %>
                    <form class="d-flex input-group w-auto">
                        <a href="/login" class="btn btn-outline-light btn-lg" type="button" data-ripple-color="dark">
                            <i class="fab fa-discord"></i> Login
                        </a>
                    </form>
                <% } %>
            </div>
        </div>
    </nav>
</script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.all.min.js"></script>
<script>
var urlParams = new URLSearchParams(window.location.search);
if(urlParams.has('success') && urlParams.get('success')){
	     Swal.fire({
	      icon: 'success',
	      title: 'Succesfull...',
	      closeOnConfirm: false,
	      text: urlParams.get('message'),
	     }).then((result) => {
  if (result.isConfirmed) {
    window.location.href = urlParams.get('url') || "/?";
  };
	 })}
	 if(urlParams.has('error') && urlParams.get('error')){
	     Swal.fire({
	      icon: 'error',
	      title: 'Oops...',
	      closeOnConfirm: false,
	      text: urlParams.get('message'),
	     }).then((result) => {
      if (result.isConfirmed) {
    window.location.href = urlParams.get('url') || "/?";
  }
	})}
 </script>
<body>